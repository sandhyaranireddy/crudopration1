<h1>Users listing page</h1>
<table>
	<thead>
		<tr>
			<th> username </th>
			<th> gender </th>
		</tr>
	</thead>
	<tbody>
		<% @users.each do |user| %>
		<tr>
			<td><%= user.username %></td>
			<td><%= user.gender %></td>
			<td><%= link_to 'show', user_path(user) %></td>
			<td><%= link_to 'delete',user_path(user), method: :delete %></td>
		</tr>
		<% end %>
	</tbody>
</table>